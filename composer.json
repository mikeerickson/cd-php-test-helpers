{
    "name": "codedungeon/php-test-helpers",
    "version": "0.0.1",
    "description": "PHPUnit Test Helpers",
    "keywords": ["composer","test","php","phpunit","helper","package"],
    "homepage": "https://github.com/mikeerickson/php-test-helpers",
    "license": "MIT",
    "authors": [{
        "name": "Mike Erickson",
        "email": "codedungeon@gmail.com"
    }],
    "type": "library",
    "require-dev": {
      "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Codedungeon\\PHPTestHelpers\\": "src/"
        }
    },
    "scripts": {
      "test":"vendor/bin/phpunit --colors=always"
    }
}
